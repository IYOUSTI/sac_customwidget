{
	"name": "customwidget_01",
	"description": "Hello World",
	"newInstancePrefix": "customwidget_01",
	"eula": "",
	"vendor": "IYOUSTI",
	"license": "",
	"id": "mycustomwidget_01",
	"version": "1.0.0",
	"icon": "https://iyousti.github.io/sac_customwidget/customwidget/icon.png",
	"webcomponents": [
		{
			"kind": "main",
			"tag": "custom-button",
			"url": "https://iyousti.github.io/sac_customwidget/customwidget/Webcomponent.js",
			"integrity": ""	,
			"ignoreIntegrity": true
			
		},
		{
			"kind": "styling",
			"tag": "custom-button-styling",
			"url": "https://iyousti.github.io/sac_customwidget/customwidget/Webcomponent-styling.js",
			"integrity": ""	,
			"ignoreIntegrity": true
			
		},
		{
			"kind": "builder",
			"tag": "custom-button-builder",
			"url": "https://iyousti.github.io/sac_customwidget/customwidget/Webcomponent-builder.js",
			"integrity": "",
			"ignoreIntegrity": true
		}	
	],
	"properties": {
		"width": {
			"type": "integer",
			"default": 192
		},
		"height": {
			"type": "integer",
			"default": 32
		},
		"opacity": {
			"type": "number",
			"description": "Opacity",
			"default": 1
			},
		"color": {
			"type": "string",
			"description": "Background color",
			"default": "red"
		}
	},
	"methods": {
		"setColor": {
		"description": "Sets the background color.",
		"parameters": [
		{
		"name": "newColor",
		"type": "string",
		"description": "The new background color"
		}
		],
		"body": "this.color = newColor;"
		},
		"getColor": {
		"returnType": "string",
		"description": "Returns the background color.",
		"body": "return this.color;"
		}
	}
	,
	"events": {
		"onClick": {
			"description": "Called when the user clicks the button."
		}
	}
}